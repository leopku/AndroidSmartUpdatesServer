
  <div class="row">
    <div class="col-sm-12">
      
<table class="table table-striped table-hover">
  <thead>
  <tr>
    <th><%= t("activerecord.attributes.application.app_id") %></th>
    <th><%= t("activerecord.attributes.application.app_name") %></th>
    <th><%= t("activerecord.attributes.application.package_name") %></th>
    <th><%= t("activerecord.attributes.application.description") %></th>
    <th>check url</th>
    <th><%= t("application.operation") %></th>
  </tr>
  </thead>
<tbody>
<% @applications.each do |application| %>
  <tr>
    <td><%= application.id %></td>
    <td><%= link_to application.app_name, application_versions_url(application)  %></td>
    <td><%= application.package_name %></td>
    <td><%= application.description %></td>
    <td><%#= link_to check_patchs_url(application, ), check_patchs_url %></td>
    <td>
      <div class="btn-group">
        <%= link_to t('application.show'), application, :class => "btn btn-default" %>
        <%= link_to t('application.edit'), edit_application_path(application), :class => "btn btn-default" %>
        <%= link_to t('application.destroy'), application, method: :delete, data: { confirm: t('application.delete_sure')},:class => "btn btn-danger" %>
      </div>
    </td>
  </tr>
<% end %>
</tbody>
</table>
  </div>
</div>
<div class="row">
  <div class="col-sm-12">
    
<%= link_to t('application.new_application'), new_application_path , :class => "btn btn-primary" %>

    </div>
  </div>
</div>

